@import "./static/fonts/iconfont.wxss";

/* 在微信小程序中不支持通配符* */
page,view,text,swiper,swiper-item,image,navigator{
  padding:0;
  margin: 0;
  box-sizing: border-box;
}
/* 主题颜色 通常变量来实现
1.less 中存在变量这个知识
2 原生的css和wxss也是支持变量
*/
page{
  /* 定义主题颜色 */
  --themeColor:#fc4141;
  /* 定义统一的字体大小，假设设计稿大小伪375px
    1px=2rpx;
    14px=28rpx;
  */
  font-size:28rpx;
}
image{
width:100%;
}